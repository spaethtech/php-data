{
    "name": "spaethtech/data",
    "description": "A simple database library.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@spaethtech.com"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "nette/php-generator": "~3.2",
        "spaethtech/common": "*",
        "spaethtech/annotations": "*",
        "spaethtech/collections": "*",
        "spaethtech/dynamics": "*"

    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "vlucas/phpdotenv": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "SpaethTech\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpaethTech\\": "./tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
